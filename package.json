{
  "name": "aggregio",
  "version": "1.0.0",
  "description": "A music service aggregator built on the MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crescit/Aggregio.git"
  },
  "keywords": [
    "music",
    "aggregator"
  ],
  "author": "Josh Jaquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crescit/Aggregio/issues"
  },
  "homepage": "https://github.com/crescit/Aggregio#readme",
  "dependencies": {
    "ajv": "6.5.4",
    "axios": "0.18.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "classnames": "2.2.6",
    "concurrently": "4.0.1",
    "express": "4.16.3",
    "jsonwebtoken": "8.3.0",
    "jwt-decode": "2.2.0",
    "moment": "2.22.2",
    "mongoose": "5.3.2",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "path": "0.12.7",
    "validator": "10.8.0"
  },
  "devDependencies": {
    "nodemon": "1.18.4"
  }
}
